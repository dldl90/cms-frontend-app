{
  "name": "cms-frontend",
  "version": "1.0.0",
  "description": "NOTHS Frontend for Contentstack CMS",
  "author": "Discovery Team (Team 5)",
  "scripts": {
    "clean:client": "rm -rf ./dist/",
    "clean:server": "rm -rf ./server-production.js",
    "clean": "npm run clean:client && npm run clean:server",
    "build:client": "npm run clean:client && webpack --config ./config/webpack.production.config.js --mode production",
    "build:server": "npm run clean:server && webpack --colors --display-error-details --progress --config ./config/webpack.server.config.js --mode production",
    "build": "npm run build:client && npm run build:server",
    "dev": "npm run clean:client && webpack --colors --display-error-details --progress --config ./config/webpack.dev.config.js --progress && nodemon server-dev.js",
    "server": "node server-production.js",
    "lint": "eslint src",
    "lint:css": "stylelint './src/**/*.js'",
    "test": "JEST_JUNIT_OUTPUT='./test-results.xml' jest --verbose --coverage --reporters=default --reporters=jest-junit",
    "features": "wait-on tcp:3000 && cypress open",
    "test:startdev": "concurrently --kill-others \"npm:dev\"",
    "test:visual": "backstop reference && npm run test:visual:change",
    "test:visual:change": "npm run test:startdev \"wait-on tcp:3000 && backstop test\"",
    "test:visual:approve": "backstop approve",
    "test:features": "npm run test:startdev \"wait-on tcp:3000 && cypress open\"",
    "ci:test:features:install": "cypress install && cypress verify",
    "ci:test:features": "concurrently --kill-others --success first \"npm:dev\" \"wait-on tcp:3000 && cypress run --browser chrome\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.0",
    "@noths/global-components": "^0.15.1",
    "@noths/global-components-v2": "^0.0.57",
    "autoprefixer": "^9.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^2.0.0",
    "connect-datadog": "0.0.9",
    "contentstack": "^3.7.0",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.1",
    "dd-trace": "^0.10.4",
    "es6-promise": "^4.2.8",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "ignore-styles": "^5.0.1",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-on-screen": "^2.1.1",
    "react-redux": "^6.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.2.0",
    "styled-components": "^4.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "devDependencies": {
    "@noths/code-style": "^0.0.17",
    "@noths/eslint-config": "^0.0.14",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-eslint": "8",
    "babel-jest": "^23.6.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-styled-components": "^1.10.6",
    "backstopjs": "^3.9.13",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "concurrently": "^4.1.1",
    "cypress": "^3.4.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-find-rules": "^3.3.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jest-junit": "^6.4.0",
    "jest-styled-components": "^6.3.3",
    "nodemon": "^1.19.1",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/cypress/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "./config/setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/src/utils/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/src/utils/__mocks__/fileMock.js"
    }
  }
}
